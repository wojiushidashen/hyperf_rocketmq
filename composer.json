{
    "name": "ezijing/rocketmq-hyperf",
    "type": "library",
    "description": "The PHP library for hyperf rocketmq",
    "keywords": [
        "rocketmq"
    ],
    "homepage": "https://github.com/thefair-net/hyperf_rocketmq",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "hyperf/guzzle": "2.2.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "hyperf/devtool": "2.2.*",
        "hyperf/testing": "2.2.*",
        "hyperf/watcher": "2.2.*",
        "swoole/ide-helper": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "TheFairLib\\RocketMQ\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "hyperf": {
            "config": "TheFairLib\\RocketMQ\\ConfigProvider"
        }
    }
}
